{
  "kind": "collectionType",
  "collectionName": "categories",
  "info": {
    "singularName": "category",
    "pluralName": "categories",
    "displayName": "Category",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "products": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::product.product",
      "mappedBy": "category"
    },
    "description": {
      "type": "dynamiczone",
      "components": [
        "about-category.about-category"
      ],
      "max": 1
    },
    "img": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "seoInfo": {
      "displayName": "seoInfo",
      "type": "component",
      "repeatable": false,
      "component": "seo-info.seo-info"
    }
  }
}
